{
	"name": "PL_SCHEMA_TEST_COPY_DATA_ORACLE_TO_ADLS",
	"properties": {
		"activities": [
			{
				"name": "Oracle to ADLS",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "OracleSource",
						"partitionOption": "None",
						"queryTimeout": "02:00:00"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "DS_ORACLE_LOCAL",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_ORACLE_SINK_ADLS",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Oracle"
		},
		"annotations": []
	}
}